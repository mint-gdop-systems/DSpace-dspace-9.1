<?xml version="1.0"?>
<dspace-dc-types>

  <dspace-header>
    <title>Custom CRVS Types Registry</title>    
  </dspace-header>

  <dc-schema>
    <name>crvs</name>
    <namespace>https://example.com/</namespace>
  </dc-schema>

  <dc-type>
    <schema>crvs</schema>
    <element>identifier</element>
    <qualifier>houseFamilyKey</qualifier>
    <scope_note>Unique composite ID: House Number plus Family Head Name.</scope_note>
  </dc-type>

  <dc-type>
    <schema>crvs</schema>
    <element>head</element>
    <qualifier>husband</qualifier>
    <scope_note>Full name of the husband/head of household.</scope_note>
  </dc-type>

  <dc-type>
    <schema>crvs</schema>
    <element>head</element>
    <qualifier>wife</qualifier>
    <scope_note>Full name of the wife.</scope_note>
  </dc-type>

  <dc-type>
    <schema>crvs</schema>
    <element>head</element>
    <qualifier>additional</qualifier>
    <scope_note>Full names of any additional house heads.</scope_note>
  </dc-type>

  <dc-type>
    <schema>crvs</schema>
    <element>head</element>
    <qualifier>nationalID</qualifier>
    <scope_note>National Digital ID of the household head.</scope_note>
  </dc-type>

  <dc-type>
    <schema>crvs</schema>
    <element>date</element>
    <qualifier>registration</qualifier>
    <scope_note>The date the family was originally registered at the Woreda level.</scope_note>
  </dc-type>

  <dc-type>
    <schema>crvs</schema>
    <element>legacy</element>
    <qualifier>serialNumber</qualifier>
    <scope_note>Serial number from the old paper ledger.</scope_note>
  </dc-type>

  <dc-type>
    <schema>crvs</schema>
    <element>family</element>
    <qualifier>count</qualifier>
    <scope_note>Total count of family members.</scope_note>
  </dc-type>

  <dc-type>
    <schema>crvs</schema>
    <element>description</element>
    <qualifier>summary</qualifier>
    <scope_note>General family description or summary.</scope_note>
  </dc-type>

  <dc-type>
    <schema>crvs</schema>
    <element>address</element>
    <qualifier>phone</qualifier>
  </dc-type>

  <dc-type>
    <schema>crvs</schema>
    <element>address</element>
    <qualifier>pobox</qualifier>
  </dc-type>

  <dc-type>
    <schema>crvs</schema>
    <element>address</element>
    <qualifier>fax</qualifier>
  </dc-type>

  <dc-type>
    <schema>crvs</schema>
    <element>address</element>
    <qualifier>email</qualifier>
  </dc-type>

  <dc-type>
    <schema>crvs</schema>
    <element>ownership</element>
    <qualifier>type</qualifier>
    <scope_note>Private (owner/other), Government, Rent, or Other.</scope_note>
  </dc-type>

</dspace-dc-types>